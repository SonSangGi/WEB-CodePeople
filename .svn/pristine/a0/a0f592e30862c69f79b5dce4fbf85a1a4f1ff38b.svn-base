 @charset "UTF-8";

.courses-photo {
	height:100px;
	width:200px;
    margin: auto;
}

.paid-home-courses-banner-java {
    background: -webkit-linear-gradient(left, rgba(255,115,115,1) 0%, rgba(175,238,238,1) 100%);
    background: -o-linear-gradient(left, rgba(255,115,115,1) 0%, rgba(175,238,238,1) 100%);
    background: linear-gradient(to right, rgba(255,115,115,1) 0%, rgba(175,238,238,1) 100%);
    min-height: 200px;
    margin-top:20px;
}

.paid-home-courses-banner-javascript {
	background: linear-gradient(90deg, rgba(189,118,162,1) 21%, rgba(255,230,98,1) 87%);
    min-height: 200px;
    margin-top:20px;
}

.paid-home-courses-banner-spring {
background: linear-gradient(90deg, rgba(255,233,131,1) 21%, rgba(98,255,206,1) 87%);
    min-height: 200px;
    margin-top:20px;
}

#paid-home-courses-photo {
	height: 100%;
	width: 100%;
	object-fit: contain;
    text-align:center;
}

#paid-home-courses-title {
	margin-top: 10px;
    font-weight: 600;
}

#paid-home-courses-price {
    margin-top: 70px;
	font-size: 20px;
    font-weight: 500;
    text-align: right;
}

#paid-home-panel-top-title {
    text-align: left;
    margin-top: 20px;
    font-weight: 800;
}

#paid-home-panel-top-instructor {
    color: slategray;
    font-size: 10px;
}
    
.checked {
  color: orange;
}
    
#paid-home-panel-top-price {
    font-size: 18px;
    font-weight: 800;
    text-align: right;
}
    
.paid-home-top-recommend li {
    padding : 20px;
    margin-right: 20px;
    float: left;
}
        
.paid-home-top-recommend {
margin-top: 30px;
}

.paid-home-top-recommend li:hover {
    background-color: #ff7373;
}

.paid-home-top-recommend li a{
    color: black;
    text-decoration: none;
}
    
    
.paid-home-courses-contents-list {
    margin-top: 50px;
}

#paid-home-courses-category {
    margin-top:50px;
    color: white;
}
        
